{
    "name": "codicastudio/youtube",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/youtube",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0",
        "google/apiclient": "^2.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dawson\\Youtube\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dawson\\Youtube\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Dawson\\Youtube\\YoutubeServiceProvider"
            ],
            "aliases": {
                "Youtube": "Dawson\\Youtube\\Facades\\Youtube"
            }
        }
    }
}
